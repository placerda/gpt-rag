{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName":                         { "value": "${AZURE_ENV_NAME}" },
    "location":                                { "value": "${AZURE_LOCATION}" },
    "deploymentTags":                          { "value": {} },

    "configureRBAC":                           { "value": "${AZURE_CONFIGURE_RBAC}" },
    
    "AIFoundryHubReuse":                       { "value": "${AZURE_REUSE_AI_FOUNDRY_HUB}" },
    "existingAIFoundryHubResourceGroupName":   { "value": "${AZURE_EXISTING_AI_FOUNDRY_HUB_RESOURCE_GROUP}" },
    "existingAIFoundryHubName":                { "value": "${AZURE_EXISTING_AI_FOUNDRY_HUB_NAME}" },

    "reuseAPIM":                               { "value": "${AZURE_REUSE_APIM}" },
    "existingAPIMResourceGroup":               { "value": "${AZURE_EXISTING_APIM_RESOURCE_GROUP}" },
    "existingAPIMName":                        { "value": "${AZURE_EXISTING_APIM_NAME}" },

    "reuseAOAI":                               { "value": "${AZURE_REUSE_AOAI}" },
    "existingAOAIResourceGroup":               { "value": "${AZURE_EXISTING_AOAI_RESOURCE_GROUP}" },
    "existingAOAIName":                        { "value": "${AZURE_EXISTING_AOAI_NAME}" },

    "frontEndContainerImage":                  { "value": "${AZURE_FRONTEND_IMAGE}" },
    "orchestratorImage":                       { "value": "${AZURE_ORCHESTRATOR_IMAGE}" },
    "dataIngestContainerImage":                { "value": "${AZURE_DATA_INGESTION_IMAGE}" },

    "AIFoundryStorageAccountName":             { "value": "${AZURE_AI_FOUNDRY_STORAGE_ACCOUNT_NAME}" },
    
    "AIHubName":                               { "value": "${AZURE_AI_FOUNDRY_HUB_NAME}" },
    "AIProjectName":                           { "value": "${AZURE_AI_FOUNDRY_PROJECT_NAME}" },
