    "embeddingsDeploymentType":                { "value": "${AZURE_EMBEDDINGS_DEPLOYMENT_TYPE}" },
    "embeddingsModelName":                     { "value": "${AZURE_OPENAI_EMBEDDING_MODEL_NAME}" },
    "embeddingsModelVersion":                  { "value": "${AZURE_EMBEDDINGS_MODEL_VERSION}" },
    "embeddingsVectorDimensions":              { "value": "${AZURE_EMBEDDINGS_VECTOR_DIMENSIONS}" },

    "chunkingMinChunkSize":                    { "value": "${AZURE_CHUNKING_MIN_CHUNK_SIZE}" },
    "chunkingNumTokens":                       { "value": "${AZURE_CHUNKING_NUM_TOKENS}" },
    "chunkingTokenOverlap":                    { "value": "${AZURE_CHUNKING_TOKEN_OVERLAP}" },
    "chatNumTokens":                           { "value": "${AZURE_CHAT_NUM_TOKENS}" }
