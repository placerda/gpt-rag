{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "deploymentTags": {
      "value": {} 
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "aiExistingProjectConnectionString": {
      "value": "${AI_EXISTING_PROJECT_CONNECTION_STRING}"
    },
    "aiHubName": {
      "value": "${AZURE_AI_HUB_NAME}"
    },
    "aiProjectName": {
      "value": "${AZURE_AI_PROJECT_NAME}"
    },
    "foundryHubReuse": {
      "value": "${AZURE_FOUNDRY_HUB_REUSE}"
    },
    "existingFoundryHubResourceGroupName": {
      "value": "${AZURE_EXISTING_FOUNDRY_HUB_RESOURCE_GROUP_NAME}"
    },
    "existingFoundryHubName": {
      "value": "${AZURE_EXISTING_FOUNDRY_HUB_NAME}"
    },
    "aiServicesConnectionName": {
      "value": "${AZURE_AI_SERVICES_CONNECTION_NAME}"
    },
    "aiServicesContentSafetyConnectionName": {
      "value": "${AZURE_AI_SERVICES_CONTENT_SAFETY_CONNECTION_NAME}"
    },
    "searchConnectionName": {
      "value": "${AZURE_SEARCH_CONNECTION_NAME}"
    },
    "aiSearchIndexName": {
      "value": "${AZURE_AI_SEARCH_INDEX_NAME}"
    },
    "logAnalyticsWorkspaceName": {
      "value": "${AZURE_LOG_ANALYTICS_WORKSPACE_NAME}"
    },
    "azureReuseConfig": {
      "value": {
        "cosmosDbReuse": "${COSMOS_DB_REUSE}",
        "appInsightsReuse": "${APP_INSIGHTS_REUSE}",
        "appServicePlanReuse": "${APP_SERVICE_PLAN_REUSE}",
        "appServiceReuse": "${APP_SERVICE_REUSE}",
        "dataIngestionFunctionAppReuse": "${DATA_INGESTION_FUNCTION_APP_REUSE}",
        "aiServicesReuse": "${AI_SERVICES_REUSE}",
        "aoaiReuse": "${AOAI_REUSE}",
        "storageReuse": "${STORAGE_REUSE}",
        "keyVaultReuse": "${KEY_VAULT_REUSE}",
        "aiSearchReuse": "${AI_SEARCH_REUSE}"
      }
    },
    "azureDbConfig": {
      "value": {
        "dbAccountName": "${AZURE_DB_ACCOUNT_NAME}",
        "dbDatabaseName": "${AZURE_DB_DATABASE_NAME}",
        "conversationContainerName": "${AZURE_DB_CONVERSATIONS_CONTAINER_NAME}",
        "datasourcesContainerName": "${AZURE_DB_DATASOURCES_CONTAINER_NAME}"
      }
    },
    "funcAppRuntimeVersion": {
      "value": "${AZURE_FUNC_APP_RUNTIME_VERSION}"
    },
    "appServiceRuntimeVersion": {
      "value": "${AZURE_APP_SERVICE_RUNTIME_VERSION}"
    },
    "appConfigName": {
      "value": "${AZURE_APP_CONFIG_NAME}"
    },
    "containerRegistryName": {
      "value": "${AZURE_CONTAINER_REGISTRY_NAME}"
    },
    "dataIngestFunctionAppName": {
      "value": "${AZURE_DATA_INGEST_FUNCTION_APP_NAME}"
    },
    "keyVaultName": {
      "value": "${AZURE_KEY_VAULT_NAME}"
    },
    "appInsightsName": {
      "value": "${AZURE_APP_INSIGHTS_NAME}"
    },
    "appServicePlanName": {
      "value": "${AZURE_APP_SERVICE_PLAN_NAME}"
    },
    "frontEndAppServiceName": {
      "value": "${AZURE_FRONT_END_APP_SERVICE_NAME}"
    },
    "storageAccountName": {
      "value": "${AZURE_STORAGE_ACCOUNT_NAME}"
    },
    "aiServicesName": {
      "value": "${AZURE_AI_SERVICES_NAME}"
    },
    "openAiServiceName": {
      "value": "${AZURE_OPENAI_SERVICE_NAME}"
    },
    "searchServiceName": {
      "value": "${AZURE_SEARCH_SERVICE_NAME}"
    }
  }
}
