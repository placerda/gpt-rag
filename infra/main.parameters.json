    "orchestratorContainerAppName":            { "value": "${AZURE_ORCHESTRATOR_CONTAINER_APP_NAME}" },
    "searchIndexName":                         { "value": "${AZURE_SEARCH_INDEX_NAME}" },
    "searchServiceName":                       { "value": "${AZURE_SEARCH_SERVICE_NAME}" },
    "solutionStorageAccountName":              { "value": "${AZURE_STORAGE_ACCOUNT_NAME}" },
    "storageAccountContainerDocs":             { "value": "${AZURE_STORAGE_ACCOUNT_CONTAINER_DOCS}" },
    "storageAccountContainerImages":           { "value": "${AZURE_STORAGE_ACCOUNT_CONTAINER_IMAGES}" },

    "apimPublisherEmail":                      { "value": "${AZURE_APIM_PUBLISHER_EMAIL}" },
    "apimPublisherName":                       { "value": "${AZURE_APIM_PUBLISHER_NAME}" },
    "apimSku":                                 { "value": "${AZURE_APIM_SKU}" },
